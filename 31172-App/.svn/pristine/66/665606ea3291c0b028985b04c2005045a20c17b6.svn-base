</body>
<script>
    apiready=function(){
        var p01=api.pageParam.name;
        var p01_use=document.getElementById('p01_use');
        p01_use.innerHTML=p01;
    }
    var btn01=document.getElementById('btn01');
    btn01.onclick=function(){
        if (p1.innerHTML=='已关注') {
            var a=function(ret){
            api.confirm({
                title:'提示',
                msg:'是否不再关注',
                buttons:['确定','取消']
                
            },function(ret){
                if(ret.buttonIndex==1){
                    p1.innerHTML='关注TA';
            btn01.style.backgroundColor='rgb(71, 43, 228)'; 
                }
            
        });
    }; 
        a();
            
        } else if (p1.innerHTML == '关注TA') {
            p1.innerHTML='已关注';
            btn01.style.backgroundColor='#aaa';
        }
        
    }     
    
    
</script>
